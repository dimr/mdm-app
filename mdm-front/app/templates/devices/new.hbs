{{outlet}}
<h1>New device</h1>

<form>
  <div class="form-group">
    <label for="serial_number">Serial number</label>
    <Input @type="text" class="form-control" @id="serial_number" @value={{serial_number}} />
  </div>
  <div class="form-group">
    <label for="type">Type</label>
    <Input @type="text" class="form-control" @id="Type"  @value={{type}} />
  </div>
  <div class="form-group">
    <label for="email">Employee id</label>
    <Input @type="text" class="form-control" @id="employee_id"  @value={{employee_id}} />
  </div>
  <button {{action "add_device" @model}} type="submit">
    Send
  </button>
</form>